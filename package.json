{
  "name": "splat-web",
  "version": "0.0.0",
  "description": "A wasm-compiled fork of splat: an RF Signal Propagation Loss, And Terrain analysis tool",
  "homepage": "https://github.com/peytondmurray/splat-web",
  "bugs": {
    "url": "https://github.com/peytondmurray/splat-web/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peytondmurray/splat-web.git"
  },
  "license": "MIT",
  "author": "Peyton Murray",
  "type": "module",
  "exports": {
    "./splat": {
      "types": "./dist/splat.d.ts",
      "default": "./dist/splat.js"
    },
    "./srtm2sdf": {
      "types": "./dist/srtm2sdf.d.ts",
      "default": "./dist/srtm2sdf.js"
    }
  },
  "files": [
    "dist/splat.js",
    "dist/splat.d.ts",
    "dist/splat.wasm",
    "dist/srtm2sdf.js",
    "dist/srtm2sdf.d.ts",
    "dist/srtm2sdf.wasm"
  ],
  "scripts": {
    "setup": "meson setup dist --cross-file=emscripten-cross.ini",
    "compile": "meson compile -C dist",
    "build": "npm run setup && npm run compile"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
